def void main() {
    let int u = std::input::get_int();
    let int r = std::rand::int_range(0, 10000 - 1);
    let list a = [0] * 10000;
    for i in std::iter::range(0, 10000, 1) {
        for j in std::iter::range(0, 100000, 1) {
            std::list::add_in_place(a, i, j % u);
        }
        std::list::add_in_place(a, i, r);
    }
    std::print(std::list::get(a, r));
}
